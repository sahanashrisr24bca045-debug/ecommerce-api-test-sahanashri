					"script": {
						"exec": [
							"pm.test(\"Status code verify\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"status message verify\",function()\r",
							"{\r",
							"    pm.response.to.have.status(\"OK\");\r",
							"});\r",
							"pm.test(\"status code below verify\",function()\r",
							"{\r",
							"    pm.expect(pm.response.code).to.be.above(199);\r",
							"})\r",
							"pm.test(\"verify response time\",function()\r",
							"{\r",
							"    pm.expect(pm.response.responseTime).to.be.below(9000)\r",
							"})\r",
							"pm.expect(\"verify response time\",function()\r",
							"{\r",
							"    pm.expect(pm.response.responseTime).to.be.above(0);\r",
							"\r",
							"})\r",
							"pm.test(\"check Multiple headers\",function()\r",
							"{\r",
							"    data=[\"content-Length\",\"content-Type\",\"Date\"];\r",
							"    data.forEach( h=>\r",
							"    {\r",
							"        pm.response.to.have.header(h);\r",
							"\r",
							"    })\r",
							"});\r",
							"pm.test(\"Test status code using if-else\",function()\r",
							"{\r",
							"    let data=pm.response.code;\r",
							"    if(data===200)\r",
							"    {\r",
							"        pm.response.to.have.status(\"OK\");\r",
							"    }\r",
							"    else\r",
							"    {\r",
							"        pm.expect(data).to.eql(\"Not Found\");\r",
							"\r",
							"    }\r",
							"});\r",
							"pm.test(\"status code is not in oneof function \",function()\r",
							"{\r",
							"    pm.expect(pm.response.code).to.not.oneOf([201,202,203,204,205,206,207]);\r",
							"\r",
							"})\r",
							"pm.test(\"content-Length value check\",function()\r",
							"{\r",
							"    let data=pm.response.headers.get(\"Content-Length\");\r",
							"    pm.expect(Number(data)).to.be.above(0);\r",
							"\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Length\",function()\r",
							"{\r",
							"    let data=pm.response.headers.get(\"Content-Length\");\r",
							"    pm.expect(Number(data)).to.be.below(8000);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response exist or not\",function()\r",
							"{\r",
							"    pm.expect(pm.response.text()).to.not.be.empty;\r",
							"})"
						],
